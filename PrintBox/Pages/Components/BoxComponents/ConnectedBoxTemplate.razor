

<MudCard Class="mx-4 pa-4">
    <MudCardHeader>
        <CardHeaderContent>
            <MudText Typo="Typo.h3">@boxName</MudText>
        </CardHeaderContent>
        <CardHeaderActions>
            <MudIconButton Icon="@Icons.Filled.SpaceDashboard" Color="Color.Primary" @onclick="@(x => RouteWithName(@boxName))" Size="Size.Large" />
        </CardHeaderActions>
    </MudCardHeader>
    <MudCardMedia Image="https://media.istockphoto.com/photos/white-box-3d-rendering-picture-id1195404252?k=20&m=1195404252&s=170667a&w=0&h=Zbhtwk5ebHVzI6UTPG4HU6S6v_HSypXL7Smj1gZG2R0=" Height="250" />
</MudCard>













@code {
	[Inject]
	public NavigationManager Navigationmanager { get; set; }


	[Parameter]
	public string boxName { get; set; }

	[Parameter]
	public EventCallback<string> ondeletedbox { get; set; }

	private void DeleteBoxFunction()
	{
		ondeletedbox.InvokeAsync("Deleted");


	}

	[Parameter]
	public EventCallback<string> onopenedbox { get; set; }
	private void OpenBoxFunction()
	{
		onopenedbox.InvokeAsync("Open");


	}


	public void RouteWithName(string linkName)
	{
		Navigationmanager.NavigateTo("/Dashboard/" + linkName);
	}

}